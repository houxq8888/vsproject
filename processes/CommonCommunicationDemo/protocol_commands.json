      "name": "SetParameter",
      "metadata": {
        "require_ack": true,
        "timeout_ms": 1000
      },
      "request_fields": [
        {
          "name": "param_id",
          "type": "uint16",
          "field_num": 1,
          "description": "Parameter identifier"
        },
        {
          "name": "value",
          "type": "float32",
          "field_num": 2,
          "description": "Parameter value"
        },
        {
          "name": "value1",
          "type": "uint8",
          "field_num": 3,
          "description": "Parameter value"
        },
        {
          "name": "value2",
          "type": "uint32",
          "field_num": 4,
          "description": "Parameter value"
        },
        {
          "name": "value3",
          "type": "bool",
          "field_num": 5,
          "description": "Parameter value"
        },
        {
          "name": "value4",
          "type": "int8",
          "field_num": 6,
          "description": "Parameter value"
        },
        {
          "name": "value5",
          "type": "int16",
