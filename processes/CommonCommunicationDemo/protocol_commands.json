{
  "protocol_version": "1.0",
  "commands": [
    {
      "id": 1,
      "name": "GetVersion",
      "metadata": {
        "require_ack": true
      },
      "response_fields": [
        {
          "name": "major",
          "type": "uint8",
          "field_num": 1,
          "default": 0
        },
        {
          "name": "minor",
          "type": "uint8",
          "field_num": 2,
          "default": 0
        },
        {
          "name": "patch",
          "type": "uint8",
          "field_num": 3,
          "default": 0
        }
      ]
    },
    {
